#include<stdio.h>
int main(){
    int arr[4]={3,4,5,6};
    int arr2[2];
    
    for(int i=0;i<5;i++){
        for(int j=0;j<5;j++){
            if(arr[i]+arr[j]== 7 && i != j)
            {
                printf("[%d,%d]",i,j);
                break;
            }
         } 
     }
     return 0;
}
